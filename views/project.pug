extends layout.pug

block content
	nav.grid-container.portfolio-breadcrumb
		a(href='/') ‚Üê Back
	article.grid-container.portfolio-intro.portfolio-project
		.grid-x
			.cell.small-6.medium-9.large-9
				h1= projectName
				p.lead.text-light= projectDescription
			.cell.portfolio-meta.small-6.medium-2.large-3
				h6 Technologies
				ul
					each val in projectTechnologies
						li= val
				a.button.expanded(href=`${liveLink}`) Live Demo
				a.button.expanded(href=`${githubLink}`) GitHub Repo
		img.image(src=projectImage[1])
		img.image(src=projectImage[2])
